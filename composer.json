{
    "name": "apikr/apikr",
    "description": "Korean API Services assembly.",
    "keywords": [
        "api",
        "rest"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.2",
        "psr/simple-cache": "^1.0",
        "mtdowling/jmespath.php": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "symfony/cache": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Apikr\\": "src/"
        }
    },
    "replace": {
        "apikr/api": "self.version",
        "apikr/aligo-sms": "self.version",
        "apikr/apistore-sms": "self.version",
        "apikr/paygate-seyfert": "self.version",
        "apikr/skplanet-tmap": "self.version"
    }
}
